
public class ListaEncadeadaExecute {

	public static void main(String[] args) {
		
		
		LinkedList Lista = new LinkedList(null,null);
		
		NodeList one = new NodeList(1);
		NodeList two = new NodeList(2);
		NodeList three = new NodeList(3);
		NodeList four = new NodeList(4);
		
		
		Lista.Insert(one);
		Lista.Insert(two);
		Lista.Insert(three);
		Lista.Insert(four);
		
		System.out.println(Lista.first.getValue());
		System.out.println(Lista.first.getNext().getValue());
		System.out.println(Lista.last.getBefore().getValue());
		System.out.println(Lista.last.getValue());
		
		
